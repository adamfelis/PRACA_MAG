<UserControl x:Class="DiagramTool.Content.TabContent"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:oxy="http://oxyplot.codeplex.com"
             >
    <Grid Name="grid">
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <oxy:Plot   Grid.Row="0" Grid.Column="0" x:Name="LongitudinalPlot" Title="A Graph" Model="{Binding LongitudinalPlotModel}" Margin="10"/>
        <oxy:Plot  Grid.Row="1" Grid.Column="0" x:Name="LateralPlot" Title="A Graph 2" Model="{Binding LateralPlotModel}" Margin="10"/>

    </Grid>
</UserControl>
