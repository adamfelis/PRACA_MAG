clc
clear
format
y0 = zeros(9, 1);
y0(1) = 1;
y0(4) = 1;
y0(9) = 1;
tspan = [0 10];
[t,y] = ode45(@vdp2,tspan,y0);
size(y)
figure
quiver3(y(:,1) * ,y(:,2),y(:,3),   y(:,1),y(:,2),y(:,3))
view(-35,45)

